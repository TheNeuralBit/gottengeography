                        <property name="is_important">True</property>
                        <accelerator key="w" signal="clicked" modifiers="GDK_CONTROL_MASK"/>
                      </object>
                    </child>
                    <child>
                      <object class="GtkToolButton" id="revert_button">
                        <property name="visible">True</property>
                        <property name="sensitive">False</property>
                        <property name="border_width">0</property>
                        <property name="stock_id">gtk-revert-to-saved</property>
                        <property name="has_tooltip">True</property>
                        <property name="tooltip_text" translatable="yes">Reload selected photos, losing all changes (Ctrl Z)</property>
                        <property name="is_important">True</property>
                        <accelerator key="z" signal="clicked" modifiers="GDK_CONTROL_MASK"/>
                      </object>
                    </child>
                    <child>
                      <object class="GtkToolButton" id="jump_button">
                        <property name="visible">True</property>
                        <property name="sensitive">False</property>
                        <property name="border_width">0</property>
                        <property name="stock_id">gtk-jump-to</property>
                        <property name="has_tooltip">True</property>
                        <property name="tooltip_text" translatable="yes">Center the map view on the selected photo (Ctrl J)</property>
                        <accelerator key="j" signal="clicked" modifiers="GDK_CONTROL_MASK"/>
                      </object>
                    </child>
                    <child>
                      <object class="GtkSeparatorToolItem" id="spacer1">
                        <property name="visible">True</property>
                        <property name="can_focus">False</property>
                        <property name="draw">False</property>
                      </object>
                      <packing>
                        <property name="expand">True</property>
                      </packing>
                    </child>
                    <child>
                      <object class="GtkToolButton" id="save_button">
                        <property name="visible">True</property>
                        <property name="sensitive">False</property>
                        <property name="can_focus">False</property>
                        <property name="has_tooltip">True</property>
                        <property name="tooltip_text" translatable="yes">Save all photos (Ctrl S)</property>
                        <property name="is_important">True</property>
                        <property name="label" translatable="yes">Save All</property>
                        <property name="use_underline">True</property>
                        <property name="stock_id">gtk-save</property>
                        <accelerator key="s" signal="clicked" modifiers="GDK_CONTROL_MASK"/>
                      </object>
                    </child>
                    <child>
                      <object class="GtkToolButton" id="apply_button">
                        <property name="visible">True</property>
                        <property name="sensitive">False</property>
                        <property name="border_width">0</property>
                        <property name="stock-id">gtk-apply</property>
                        <property name="has_tooltip">True</property>
                        <property name="tooltip_text" translatable="yes">Place selected photos onto center of map (Ctrl Enter)</property>
                        <accelerator key="Return" signal="clicked" modifiers="GDK_CONTROL_MASK"/>
                      </object>
                    </child>
                  </object>
                </child>
              </object>
              <packing>
                <property name="position">0</property>
              </packing>
            </child>
            <child type="tab">
              <object class="GtkLabel" id="tab_photo">
                <property name="can_focus">False</property>
                <property name="label" translatable="yes">Photos</property>
                <property name="single_line_mode">True</property>
                <property name="hexpand">True</property>
              </object>
              <packing>
                <property name="position">0</property>
              </packing>
            </child>
            <child>
              <object class="GtkScrolledWindow" id="cameras_scroller">
                <property name="can_focus">False</property>
                <property name="hscrollbar_policy">never</property>
                <property name="vscrollbar_policy">automatic</property>
                <property name="shadow_type">none</property>
                <property name="kinetic_scrolling">False</property>
                <property name="expand">True</property>
                <property name="border_width">0</property>
                <child>
                  <object class="GtkViewport" id="cameras_viewport">
                    <property name="can_focus">False</property>
                    <child>
                      <object class="GtkGrid" id="cameras_view">
                        <property name="can_focus">False</property>
                        <property name="margin">20</property>
                        <property name="row-spacing">20</property>
                        <property name="column-spacing">6</property>
                        <property name="column-homogeneous">True</property>
                        <property name="halign">center</property>
                        <property name="valign">center</property>
                        <property name="orientation">vertical</property>
                        <child>
                          <object class="GtkLabel" id="empty_camera_list">
                            <property name="sensitive">True</property>
                            <property name="no_show_all">True</property>
                            <property name="visible">True</property>
                            <property name="can_focus">False</property>
                            <property name="wrap">True</property>
                            <property name="label" translatable="yes">Drag files here to load.</property>
                          </object>
                        </child>
                      </object>
                    </child>
                  </object>
                </child>
              </object>
              <packing>
                <property name="position">1</property>
              </packing>
            </child>
            <child type="tab">
              <object class="GtkLabel" id="tab_cameras">
                <property name="can_focus">False</property>
                <property name="label" translatable="yes">Cameras</property>
                <property name="single_line_mode">True</property>
                <property name="hexpand">True</property>
              </object>
              <packing>
                <property name="position">1</property>
              </packing>
            </child>
            <child>
              <object class="GtkScrolledWindow" id="trackfile_scroller">
                <property name="can_focus">False</property>
                <property name="hscrollbar_policy">never</property>
                <property name="vscrollbar_policy">automatic</property>
                <property name="shadow_type">none</property>
                <property name="kinetic_scrolling">False</property>
                <property name="expand">True</property>
                <property name="border_width">0</property>
                <child>
                  <object class="GtkViewport" id="trackfile_viewport">
                    <property name="can_focus">False</property>
                    <child>
                      <object class="GtkGrid" id="trackfiles_view">
                        <property name="margin">20</property>
                        <property name="row-spacing">12</property>
                        <property name="column-spacing">6</property>
                        <property name="column-homogeneous">True</property>
                        <property name="halign">center</property>
                        <property name="valign">center</property>
                        <property name="orientation">vertical</property>
                        <child>
                          <object class="GtkLabel" id="empty_trackfile_list">
                            <property name="sensitive">True</property>
                            <property name="no_show_all">True</property>
                            <property name="visible">True</property>
                            <property name="can_focus">False</property>
                            <property name="wrap">True</property>
                            <property name="label" translatable="yes">Drag files here to load.</property>
                          </object>
                        </child>
                      </object>
                    </child>
                  </object>
                </child>
              </object>
              <packing>
                <property name="position">2</property>
              </packing>
            </child>
            <child type="tab">
              <object class="GtkLabel" id="tab_gps">
                <property name="can_focus">False</property>
                <property name="label" translatable="yes">GPS</property>
                <property name="single_line_mode">True</property>
                <property name="hexpand">True</property>
              </object>
              <packing>
                <property name="position">2</property>
              </packing>
            </child>
          </object>
          <packing>
            <property name="width">1</property>
            <property name="left-attach">0</property>
            <property name="top-attach">1</property>
          </packing>
        </child>
        <child>
          <object class="GtkScrolledWindow" id="map_border">
            <property name="can_focus">False</property>
            <property name="hscrollbar_policy">never</property>
            <property name="vscrollbar_policy">never</property>
            <property name="shadow_type">etched-out</property>
            <property name="kinetic_scrolling">False</property>
            <property name="expand">True</property>
            <property name="border_width">3</property>
            <child>
              <object class="GtkViewport" id="map_viewport">
                <property name="can_focus">False</property>
                <child>
                  <object class="GtkOverlay" id="map_container">
                    <property name="can_focus">False</property>
                    <child>
                      <placeholder/>
                    </child>
                    <child type="overlay">
                      <object class="GtkInfoBar" id="error_bar">
                        <property name="no_show_all">True</property>
                        <property name="visible">False</property>
                        <property name="can_focus">False</property>
                        <property name="expand">False</property>
                        <property name="halign">0</property>
                        <property name="valign">1</property>
                        <child internal-child="content_area">
                          <object class="GtkBox" id="error_bar_content">
                            <property name="can_focus">False</property>
                            <property name="border_width">8</property>
                            <property name="spacing">16</property>
                            <property name="visible">True</property>
                            <child>
                              <object class="GtkImage" id="error_icon">
                                <property name="can_focus">False</property>
                                <property name="visible">True</property>
                              </object>
                              <packing>
                                <property name="expand">False</property>
                                <property name="fill">False</property>
                                <property name="position">0</property>
                              </packing>
                            </child>
                            <child>
                             <object class="GtkLabel" id="error_message">
                                <property name="visible">True</property>
                                <property name="can_focus">False</property>
                                <property name="use_markup">True</property>
                                <property name="wrap">True</property>
                              </object>
                              <packing>
                                <property name="expand">True</property>
                                <property name="fill">True</property>
                                <property name="position">1</property>
                              </packing>
                            </child>
                          </object>
                        </child>
