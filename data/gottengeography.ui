<?xml version="1.0" encoding="UTF-8"?>
<interface>
  <!-- interface-requires gtk+ 3.0 -->
  <object class="GtkApplicationWindow" id="main">
    <property name="width_request">620</property>
    <property name="height_request">400</property>
    <property name="can_focus">False</property>
    <property name="show_menubar">False</property>
    <property name="title">GottenGeography</property>
    <property name="icon_name">gottengeography</property>
    <child>
      <object class="GtkGrid" id="app_container">
        <property name="can_focus">False</property>
        <child>
          <object class="GtkToolbar" id="toolbar">
            <property name="can_focus">False</property>
            <style>
                <class name="primary-toolbar"/>
            </style>
            <child>
              <object class="GtkToolButton" id="open_button">
                <property name="use_action_appearance">False</property>
                <property name="can_focus">False</property>
                <property name="has_tooltip">True</property>
                <property name="tooltip_text" translatable="yes">Load photos or GPS data (Ctrl O)</property>
                <property name="stock_id">gtk-open</property>
                <accelerator key="o" signal="clicked" modifiers="GDK_CONTROL_MASK"/>
              </object>
            </child>
            <child>
              <object class="GtkSeparatorToolItem" id="sep1">
                <property name="can_focus">False</property>
                <property name="draw">False</property>
                <property name="width_request">50</property>
              </object>
              <packing>
                <property name="expand">False</property>
              </packing>
            </child>
            <child>
              <object class="GtkMenuToolButton" id="map_source_menu_button">
                <property name="menu">map_source_menu</property>
                <property name="is_important">True</property>
                <property name="can_focus">False</property>
                <property name="tooltip_text" translatable="yes">Change map view (Google opens in your browser)</property>
                <property name="label">Google</property>
                <child type="menu">
                  <object class="GtkMenu" id="map_source_menu">
                    <property name="can_focus">False</property>
                  </object>
                </child>
              </object>
            </child>
            <child>
              <object class="GtkSeparatorToolItem" id="sep2">
                <property name="can_focus">False</property>
                <property name="draw">False</property>
                <property name="width_request">50</property>
              </object>
            </child>
            <child>
              <object class="GtkToolButton" id="back_button">
                <property name="can_focus">True</property>
                <property name="tooltip_text" translatable="yes">Return to the previous location viewed on map (Ctrl Left)</property>
                <property name="stock-id">gtk-go-back</property>
                <accelerator key="Left" signal="clicked" modifiers="GDK_CONTROL_MASK"/>
              </object>
            </child>
            <child>
              <object class="GtkToolItem" id="search_box_container">
                <child>
                  <object class="GtkEntry" id="search_box">
                    <property name="can_focus">True</property>
                    <property name="has_tooltip">True</property>
                    <property name="placeholder_text" translatable="yes">Type the name of the city you would like to view.</property>
                    <property name="width_chars">30</property>
                    <property name="caps_lock_warning">False</property>
                    <property name="secondary_icon_stock">gtk-find</property>
                    <accelerator key="l" signal="grab_focus" modifiers="GDK_CONTROL_MASK"/>
                  </object>
                </child>
              </object>
              <packing>
                <property name="expand">True</property>
              </packing>
            </child>
            <child>
              <object class="GtkToolButton" id="zoom_out_button">
                <property name="can_focus">True</property>
                <property name="tooltip_text" translatable="yes">Zoom the map out one step (Ctrl -)</property>
                <property name="stock-id">gtk-zoom-out</property>
                <accelerator key="minus" signal="clicked" modifiers="GDK_CONTROL_MASK"/>
              </object>
            </child>
            <child>
              <object class="GtkToolButton" id="zoom_in_button">
                <property name="can_focus">True</property>
                <property name="tooltip_text" translatable="yes">Enhance! (Ctrl +)</property>
                <property name="stock-id">gtk-zoom-in</property>
                <accelerator key="equal" signal="clicked" modifiers="GDK_CONTROL_MASK"/>
              </object>
            </child>
            <child>
              <object class="GtkSeparatorToolItem" id="sep3">
                <property name="can_focus">False</property>
                <property name="draw">False</property>
                <property name="width_request">50</property>
              </object>
            </child>
            <child>
              <object class="GtkToolButton" id="help_button">
                <property name="can_focus">False</property>
                <property name="has_tooltip">True</property>
                <property name="is_important">True</property>
                <property name="tooltip_text" translatable="yes">Learn how to use GottenGeography (Ctrl ?)</property>
                <property name="stock_id">gtk-help</property>
                <accelerator key="question" signal="clicked" modifiers="GDK_CONTROL_MASK"/>
                <accelerator key="slash" signal="clicked" modifiers="GDK_CONTROL_MASK"/>
              </object>
              <packing>
                <property name="expand">False</property>
                <property name="homogeneous">True</property>
              </packing>
            </child>
            <child>
              <object class="GtkToolButton" id="about_button">
                <property name="can_focus">False</property>
                <property name="has_tooltip">True</property>
                <property name="tooltip_text" translatable="yes">About GottenGeography</property>
                <property name="stock_id">gtk-about</property>
              </object>
              <packing>
                <property name="expand">False</property>
                <property name="homogeneous">True</property>
              </packing>
            </child>
          </object>
          <packing>
            <property name="width">2</property>
            <property name="left-attach">0</property>
            <property name="top-attach">0</property>
          </packing>
        </child>
        <child>
          <object class="GtkNotebook" id="photo_camera_gps">
            <property name="can_focus">False</property>
            <property name="border_width">3</property>
            <property name="expand">False</property>
            <child>
              <object class="GtkGrid" id="photos_with_toolbar">
                <property name="can_focus">False</property>
                <property name="border_width">0</property>
                <property name="orientation">vertical</property>
                <child>
                  <object class="GtkOverlay" id="photos_with_buttons">
                    <property name="can_focus">False</property>
                    <property name="expand">True</property>
                    <property name="border_width">0</property>
                    <child>
                      <object class="GtkScrolledWindow" id="photoscroller">
                        <property name="can_focus">False</property>
                        <property name="border_width">0</property>
                        <property name="hscrollbar_policy">never</property>
                        <child>
                          <object class="GtkTreeView" id="photos_view">
                            <property name="can_focus">True</property>
                            <property name="headers_visible">False</property>
                            <property name="headers_clickable">False</property>
                            <property name="enable_search">False</property>
                            <property name="show_expanders">False</property>
                            <property name="model">loaded_photos</property>
                            <child internal-child="selection">
                              <object class="GtkTreeSelection" id="photos_selection">
                                <property name="mode">GTK_SELECTION_MULTIPLE</property>
                              </object>
                            </child>
                            <child>
                              <object class="GtkTreeViewColumn" id="photos_column">
                                <property name="title">Photos</property>
                                <property name="sizing">GTK_TREE_VIEW_COLUMN_AUTOSIZE</property>
                                <child>
                                  <object class="GtkCellRendererPixbuf" id="photos_thumb_renderer">
                                    <property name="xpad">12</property>
                                    <property name="ypad">6</property>
                                  </object>
                                  <attributes>
                                    <attribute name="pixbuf">2</attribute>
                                  </attributes>
                                </child>
                                <child>
                                  <object class="GtkCellRendererText" id="photos_summary_renderer">
                                    <property name="wrap-width">200</property>
                                    <property name="wrap-mode">0</property>
                                  </object>
                                  <attributes>
                                    <attribute name="markup">1</attribute>
                                  </attributes>
                                </child>
                              </object>
                            </child>
                          </object>
                        </child>
                      </object>
                    </child>
                    <child type="overlay">
                      <object class="GtkLabel" id="empty_photo_list">
                        <property name="sensitive">True</property>
                        <property name="halign">0</property>
                        <property name="valign">0</property>
                        <property name="can_focus">False</property>
                        <property name="wrap">True</property>
