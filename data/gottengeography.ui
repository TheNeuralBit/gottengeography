                        <property name="can_focus">False</property>
                        <property name="no_show_all">True</property>
                        <property name="show_text">True</property>
                        <property name="margin">6</property>
                        <property name="halign">0</property>
                        <property name="valign">2</property>
                      </object>
                    </child>
                  </object>
                </child>
              </object>
            </child>
          </object>
          <packing>
            <property name="width">1</property>
            <property name="left-attach">1</property>
            <property name="top-attach">1</property>
          </packing>
        </child>
      </object>
    </child>
  </object>
  <object class="GtkWindow" id="large_preview_window">
    <property name="can_focus">False</property>
    <property name="title">GottenGeography</property>
    <property name="icon_name">gottengeography</property>
    <property name="window_position">center</property>
    <child>
      <object class="GtkGrid" id="large_preview_grid">
        <property name="visible">True</property>
